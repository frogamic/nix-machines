timeout 300 'swaylock-fancy --pixelate --daemonize'
timeout 360 'swaymsg "output * dpms off"' resume 'swaymsg "output * dpms on"'
timeout 600 'systemctl suspend' resume 'swaymsg "output * dpms on"'
before-sleep 'swaylock-fancy --pixelate --daemonize'
after-resume 'swaymsg "output * dpms on"'
lock 'swaylock-fancy --pixelate --daemonize'
unlock 'swaymsg "output * dpms on"'
